;; types
Char = /unsigned 8
Handle = /unsigned 32
RealWorld = /unsigned 8

stdin: Handle = 0
stdout: Handle = 1
stderr: Handle = 2

a = "a"
b: Char = "b"

fst: (Char, Char) -> Char = \(x,_) => x
foo: Char -> Char = \_ => fst("z","z")
bar: Char -> Char = \_ => fst (a,b : Char)
qux: Char -> Char = \_ => fst (c,d)
  /where
    c = "c"
    d: Char = "d"

h-get-char: Handle -> Char
get-char: RealWorld -> Char = \_ => h-get-char stdin

