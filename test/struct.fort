;; types
Char = /char 8
Int = /signed 32
Int64 = /signed 64

MyStruct = /record
  x: Int
  y: Char
  z: Int64
  
MyStruct2 = /record
  y: Int
  x: Int64

;; primitives

store: (/address a, a) -> ()
:= = store

output: a -> ()

;; code
foo-struct: (/address MyStruct, /address MyStruct2) -> () = \(mystr, mystr2) => /do
  output mystr
  x mystr := 42
  y mystr := #"c"
  z mystr := 42424242
  output mystr
  output mystr2
  y mystr2 := 24
  x mystr2 := 24242424
  output mystr2
