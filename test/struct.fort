;; types
Char = /unsigned 8
Int = /signed 32
Int64 = /signed 64

MyStruct = /record
  x: Int
  y: Char
  z: Int64
  
MyStruct2 = /record
  y: Int64
  x: Int

;; primitives

store: (/address a, a) -> ()
.= = store

;; code
foo-struct: (/address MyStruct, /address MyStruct2) -> () = \(mystr, mystr2) => /do
  x mystr .= 42
  y mystr .= "c"
  z mystr .= 42424242
  y mystr2 .= 24242424
  x mystr2 .= 24
  
  
